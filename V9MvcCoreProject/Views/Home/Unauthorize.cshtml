<script>var count =5;
    $('#count').text(count);
    var timer = setInterval(function () {
        count--;
        if (count === 0) {
            var url = "/Home/Index";
            window.location.replace(url);
        }
        $('#count').text(count);
    }, 1000); // Every second</script>